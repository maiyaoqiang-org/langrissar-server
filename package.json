{
  "name": "langrissar-server",
  "version": "1.0.0",
  "main": "dist/src/main",
  "scripts": {
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/UserMigration -d src/config/ormconfig.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/ormconfig.ts",
    "docker-run": "docker-compose up -d",
    "docker-stop": "docker-compose down",
    "docker-restart": "docker-compose restart",
    "docker-logs": "docker-compose logs -f",
    "docker-update": "docker-compose pull && docker-compose up -d"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.16",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.16",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.1.2",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^4.3.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "mysql2": "^3.14.0",
    "openai": "^4.100.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer-core": "^24.8.2",
    "reflect-metadata": "^0.2.2",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.3.22",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
